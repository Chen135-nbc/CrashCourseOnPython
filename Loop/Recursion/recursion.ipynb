   "source": [
    "def factorial(n):\n",
    "    if n < 2:   #base case\n",
    "        return 1\n",
    "    return n * factorial(n-1)   #recursion,the recursive case and this creates a loop.\n",
    "\n",
    "print(factorial(3)) #3*2\n",
    "print(factorial(4)) #4*3*2\n",
    "print(factorial(5)) #5*4*3*2\n",
    "print(factorial(6)) #6#5*4*3*2"
   ],
